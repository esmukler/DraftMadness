<div class="header">
  <div class="container">
    <div class="text-center">
      <%= image_tag 'bball-icon', class: 'bball-icon' %>
    </div>

    <div class="login-btn text-center">
      <% if user_signed_in? %>
        <%= link_to 'Logout', logout_path, class: 'btn btn-default' %>
      <% else %>
        <%= link_to 'Hello', '#', class: 'btn btn-default' %>
      <% end %>
    </div>

    <div class="text-center title">
      <% if user_signed_in? %>
        <%= link_to 'Draft Madness 2016', user_path(current_user) %>
      <% else %>
        Draft Madness 2016
      <% end %>
    </div>
  </div>
</div>

